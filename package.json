{
  "name": "task-engine-sdk",
  "version": "0.0.1",
  "repository": "https://github.com/bhuman-ai/task-engine-sdk",
  "types": "./dist/types/browser/index.d.ts",
  "exports": {
    "browser": {
      "import": "./dist/browser.es.js",
      "require": "./dist/browser.cjs.js",
      "default": "./dist/browser.es.js"
    },
    "node": {
      "import": "./dist/node.es.js",
      "require": "./dist/node.cjs.js",
      "default": "./dist/node.es.js"
    }
  },
  "scripts": {
    "build": "tsc -P tsconfig.build.json && rollup -c",
    "doc:dev": "vuepress dev doc",
    "doc:build": "vuepress build doc",
    "doc:types": "typedoc --themeColor '#187EE7' --plugin typedoc-material-theme src/index.ts",
    "dev": "ts-node -r tsconfig-paths/register"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@swc/cli": "^0.3.0",
    "@swc/core": "^1.3.106",
    "@types/ws": "^8.5.10",
    "rollup": "^4.9.6",
    "rollup-plugin-bundle-size": "^1.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.25.8",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^5.3.3",
    "vuepress": "^1.5.3",
    "ws": "^8.16.0"
  }
}
